<script src="/javascripts/utilFunc.js"></script>
<script>
    var postId = JSON.parse(htmlDecode("<%= JSON.stringify(postId) %>"));
    var disqus_config = function () {
        this.page.url = 'https://gwangit.com/posts/'+ postId;
        this.page.identifier = postId;
    };
    (function() {
        var d = document, s = d.createElement('script');
        s.src = '//gwangit.disqus.com/embed.js';
        s.setAttribute('data-timestamp', +new Date());
        (d.head || d.body).appendChild(s);
    })();
</script>
